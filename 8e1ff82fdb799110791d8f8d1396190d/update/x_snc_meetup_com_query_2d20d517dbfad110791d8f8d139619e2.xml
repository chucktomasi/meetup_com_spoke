<?xml version="1.0" encoding="UTF-8"?><record_update table="x_snc_meetup_com_query">
    <x_snc_meetup_com_query action="INSERT_OR_UPDATE">
        <active>false</active>
        <id>get-network-groups</id>
        <name>Get list of groups for the network</name>
        <query>query ($urlname: String!) {
  proNetworkByUrlname(urlname: $urlname) {
    groupsSearch {
      count
      edges {
        node {
          id
          name
          urlname
          memberships{
            count
          }
        }
      }
    }
  }
}
</query>
