    <x_snc_meetup_com_query action="INSERT_OR_UPDATE">
        <active>true</active>
        <id>get-event-details</id>
        <name>Get the details for a specific event</name>
        <query>query ($eventId: ID!) {
  event(id: $eventId) {
    title
    description
    status
    eventUrl
    tickets {
      yesCount
      noCount
      attendedCount
    }
  }
}
</query>
